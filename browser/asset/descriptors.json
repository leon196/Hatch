{
	"animations": "animation/scene.json",
	"fonts": {
	},
	"textures": {
	},
	"geometries": {
		"geo": {
			"file": "mesh/geo.ply"
		}
	},
	"shaders": {
		"dust": {
			"vertexShader": "shader/particle/dust.vert",
			"fragmentShader": "shader/color.frag"
		},
		"egg": {
			"vertexShader": "shader/mesh/egg.vert",
			"fragmentShader": "shader/mesh/egg.frag",
			"side": 2
		},
		"eggcrack": {
			"vertexShader": "shader/line/eggcrack.vert",
			"fragmentShader": "shader/color.frag",
			"side": 2
		},
		"curves": {
			"vertexShader": "shader/line/curves.vert",
			"fragmentShader": "shader/line/curves.frag",
			"side": 2
		},
		"text": {
			"vertexShader": "shader/mesh/text.vert",
			"fragmentShader": "shader/mesh/text.frag",
			"side": 2,
			"depthTest": false,
			"transparent": true
		},
		"bright": {
			"vertexShader": "shader/filter/bloom/bright.vert",
			"fragmentShader": "shader/filter/bloom/bright.frag"
		},
		"gaussian_blur": {
			"vertexShader": "shader/filter/bloom/gaussian_blur.vert",
			"fragmentShader": "shader/filter/bloom/gaussian_blur.frag"
		},
		"bloom": {
			"vertexShader": "shader/filter/bloom/bloom.vert",
			"fragmentShader": "shader/filter/bloom/bloom.frag"
		},
		"render": {
			"vertexShader": "shader/filter/fullscreen.vert",
			"fragmentShader": "shader/filter/render.frag"
		},
		"edge": {
			"vertexShader": "shader/filter/fullscreen.vert",
			"fragmentShader": "shader/filter/edge.frag"
		}
	}
}
